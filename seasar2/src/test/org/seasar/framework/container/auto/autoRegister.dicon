<?xml version="1.0" encoding="Shift_JIS"?>
<!DOCTYPE components PUBLIC "-//SEASAR2.1//DTD S2Container//EN" 
	"http://www.seasar.org/dtd/components21.dtd">
<components namespace="child" >
	<component class="org.seasar.framework.container.auto.FileSystemComponentAutoRegister">
		<property name="fileNameOfRoot">"aop.dicon"</property>
		<property name="autoNaming">
			<component class="org.seasar.framework.container.auto.DefaultAutoNaming"/>
		</property>
		<initMethod name="addClassPattern">
			<arg>"test.org.seasar.framework.container.auto"</arg>
			<arg>"Foo.*"</arg>
		</initMethod>
		<initMethod name="registAll"></initMethod>
	</component>
	<component class="org.seasar.framework.container.auto.InterceptorAutoRegister">
		<property name="interceptor">"greetingInterceptor"</property>
		<initMethod name="addClassPattern">
			<arg>"test.org.seasar.framework.container.auto"</arg>
			<arg>"Foo.*"</arg>
		</initMethod>
		<initMethod name="registAll"></initMethod>
	</component>
	<component name="greetingInterceptor" class="test.org.seasar.framework.container.auto.GreetingInterceptor"/>
</components>